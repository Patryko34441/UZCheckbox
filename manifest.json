{
  "name": "UZP",
  "author": "Patryk Dudała",
  "version": "1.0.0",
  "description": "Rozszerzenie automatycznie zaznaczające checkboxy na planie UZ.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [ "http://www.plan.uz.zgora.pl/grupy_plan.php?ID=*" ],
      "js": ["contentscript.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 3
}